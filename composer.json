{
    "name": "cakephp/app",
    "require": {
        "cakephp/cakephp": "2.10.*"
    },
    "config": {
        "vendor-dir": "Vendor/",
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "require-dev": {
        "cakephp/debug_kit": "^2.2.0",
        "cakephp/cakephp-codesniffer": "1.0.4",
        "phpstan/phpstan": "1.4",
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "App\\ApplicationService\\": "ApplicationService/",
            "App\\Domain\\": "Domain/",
            "App\\Infrastructure\\": "Infrastructure/"
        }
    }
}
